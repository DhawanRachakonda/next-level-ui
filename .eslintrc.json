{
  "extends": ["react-app"],
  "rules": {
    "react/prop-types": 0,
    "react-hooks/rules-of-hooks": "error",
    "no-console": "warn",
    "import/no-unresolved": 0
  },
  "overrides": [
    {
      "files": ["**/*.ts?(x)"],
      "rules": {
        "@typescript-eslint/no-unused-vars": "error",
        "no-console": "warn"
      }
    }
  ]
}
